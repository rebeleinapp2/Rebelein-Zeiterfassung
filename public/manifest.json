{
  "name": "Zeiterfassung Rebelein",
  "short_name": "Zeiterfassung",
  "start_url": "/",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay"
  ],
  "background_color": "#0f172a",
  "theme_color": "#0f172a",
  "orientation": "portrait",
  "id": "zeiterfassung-rebelein",
  "scope": "/",
  "description": "Erfasse deine Arbeitszeiten, Pausen und Projekte einfach und effizient.",
  "categories": [
    "productivity",
    "business",
    "utilities"
  ],
  "icons": [
    {
      "src": "/icon.svg",
      "sizes": "192x192 512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/icon.svg",
      "sizes": "192x192 512x512",
      "type": "image/svg+xml",
      "purpose": "maskable"
    },
    {
      "src": "/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/icon.svg",
      "sizes": "1024x1024",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "Desktop Ansicht"
    },
    {
      "src": "/icon.svg",
      "sizes": "1024x1024",
      "type": "image/svg+xml",
      "form_factor": "narrow",
      "label": "Mobile Ansicht"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": [
            "application/pdf"
          ]
        }
      ]
    }
  }
}